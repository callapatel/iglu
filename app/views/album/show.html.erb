<div class='container'>
  <h1><center><div class = headerdos> <%= params[:feed_id]%> Album </div></center></br></br></br></br>
  <div class="row">
    <% @albumnames = params[:feed_id] %>
<% AlbumPhoto.where(album_id: params[:feed_id]).each do |ap| %>

  <div class= 'col-xs-12 col-md-4 col-lg-4'>
    <div class = 'class'>
<% @photoid = ap.photo_id %>
<% photos = Photo.find(@photoid) %>
<%= button_to "-", deletion_path(f_id: @photoid, feed_id: @albumnames), {:class => "btn btn-primary yelpbutton prettyblue delete" } %>
<% @image = photos.url%>
<%= image_tag(@image) %>
<%= photos.ig_user %></br>
<%= photos.loc_name %></br>
<div class ='commentfrominsta'>
<%= photos.caption %></br>
<% photos.yelp_url %>
<% if photos.yelp_url != "" && photos.yelp_url != nil%>
<a href="<%= photos.yelp_url %>">
  <div class= 'btn btn-primary yelpbutton prettyblue'><i class="fa fa-yelp fa-2x"></i></div>
  <% else %>
  <div class='toolis' data-toggle="tooltip" data-placement="top"  title="yelp data not available">
    <div class= 'btn btn-primary yelpbutton prettyblue a' ><i class="fa fa-yelp fa-2x"></i></div>
  </div>
  <% end %>
</div>


  </div>
  </div>

<% end %>
</div>
</div>
