
<div class="scroll">
  <ul class="rig columns-3">

  <% @photos.each do |n|%>
  <!-- <div class='club row'> -->
  <li>
  <div class = 'club row'>
    <%= image_tag("#{n.url}")%>
  </div>
    </br>
    <%= n.ig_user %></br>

    <%= n.loc_name %></br>
    <%= n.lat %></br>
    <%= n.long %></br>
    <%= button_to "Pin It", "/pinned/new", class: 'choose btn btn-default' %>
    <% @feed = n.user %>
    <%= hidden_field_tag "[uid]", n.user, class: "info" %>

  </li>

  <% end %>

</ul>


</div>
<!-- </div> -->
